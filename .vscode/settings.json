{
    "editor.detectIndentation": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationForFirstPipeline",
    "powershell.debugging.createTemporaryIntegratedConsole": true,
    "js/ts.implicitProjectConfig.checkJs": true,
    "haskell.formattingProvider": "brittany",
    "[haskell]": {
        "editor.tabSize": 4
    },
    "[clojure]": {
        "editor.tabSize": 2
    },
    "omnisharp.autoStart": false,
    "calva.highlight.enableBracketColors": false,
    "calva.paredit.defaultKeyMap": "none",
    "calva.fmt.configPath": "${workspaceFolder}/.config/cljfmt.edn",
    "calva.showCalvaSaysOnStart": false,
    "editor.lineNumbers": "relative",
    "zenMode.hideLineNumbers": false,
    "workbench.preferredLightColorTheme": "Verdandi",
    "workbench.preferredDarkColorTheme": "minimalGreen",
    "editor.bracketPairColorization.enabled": false
}